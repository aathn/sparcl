sig split : Reader Int (List Symbol) (List Symbol, List Symbol)
def split =
  arr uncons >>>
  arrCase (liftP $ \(x, y) -> and (null x) (null y))
    (arr (\(rev ()) -> rev (rev Nil, rev Nil)))
    (localR is0 $
      caseR
        (arr (\(rev (h, t)) -> rev (rev Nil, rev Cons h t)))
        (second (localR pred split) >>>
         arr (\(rev (h, (hs, ts))) -> rev (rev Cons h hs, ts))))

sig concat : Reader Int (List Symbol, List Symbol) (List Symbol)
def concat = arrInv split

sig reverse : rev (List Symbol) -o rev (List Symbol)
def reverse l =
  let rev (l, len)  <- lengthR rev (l, rev ()) in
  let rev (Nil, l') <- goReverse len l in l'
  where
    def goReverse n l =
      case is0 n of
      | rev Left  () -> rev (l, rev Nil) with null . snd
      | rev Right n  ->
        let rev (Cons h t, l') <- goReverse (pred n) l in
        rev (t, rev Cons h l')
      end
  end

