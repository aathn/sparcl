sig length : GFun (List a) Int
def length = arrG (pairr (new eqInt 0)) >>! traceG lenStep
  where
    def lenStep =
      arrG (distr . mapFst uncons) >>!
        ((arrG unitl) ++!
         (firstG sndG >>! arrG (mapSnd succ)))
  end

