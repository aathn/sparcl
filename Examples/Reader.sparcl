sig revTake : GFun (Int, List a) (List a)
def revTake = arrG (assocr. pairr (rev Nil)) >>! goRevTake
  where
    def goRevTake =
      arrG (distr . mapFst uncons . assocr . swap) >>!
      caseG
        (arrG unitl >>! secondG erase >>! arrG unitr)
        (arrG (distr . mapFst is0 . swap . assocl) >>!
          caseG
            (arrG unitl >>! firstG erase >>! arrG unitl)
            (arrG (mapSnd (mapSnd (\(rev (h, acc)) -> rev Cons h acc)
                           . assocr . mapFst swap) . mapFst pred) >>!
             goRevTake'))

    def goRevTake' = GF (\x -> let GF f <- goRevTake in f x)
  end
