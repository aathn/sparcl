sig repeatTake : GFun (Int, List Symbol) (List Symbol)
def repeatTake = secondG (arrG $ copy (eqList eqChar)) >>! goRepeatTake
  where
    def goRepeatTake =
      arrG (swap . assocl) >>! firstG (arrG uncons) >>! arrG distr >>!
      caseG
        (arrG unitl >>! secondG (arrG $ copy (eqList eqChar)) >>! goRepeatTake')
        (arrG (distr . mapFst is0 . assocr . swap) >>!
          caseG
            (erase >>! arrG (\(rev ()) -> rev Nil))
            (arrG (swap . assocl . mapSnd (assocl . mapSnd swap)) >>!
             secondG (firstG (arrG pred) >>! goRepeatTake') >>!
             arrG (\(rev (h, rest)) -> rev Cons h rest)))

    def goRepeatTake' = GF (\x -> let GF f <- goRepeatTake in f x)
  end

