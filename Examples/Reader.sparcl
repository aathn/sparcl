sig decR : Reader Huff (List Bit) (Symbol, List Bit)
def decR =
  localR unhuff $
    caseR
      (ask eqChar)
      (arr unbit >>>
       arrCase (liftPR $ \((l, _), (s, _)) -> member s l)
         (mutateR fstG decR)
         (mutateR sndG decR))

sig encR : Reader Huff (Symbol, List Bit) (List Bit)
def encR = arrInv decR

