sig goLz77 : Reader (List Symbol) (List Symbol) (List LZPair)
def goLz77 =
  arr uncons >>>
  arrCase (liftP null)
    (arr (\(rev ()) -> rev Nil))
    (pinR eqMaybeIntPair maxMatch >>> arr (distr . mapFst unmaybe . swap) >>>
     arrCase (liftP $ isLit . head)
      (arr unitl >>> updateR updateWindow goLz77 >>>
       arr (\(rev (h, lzRest)) -> rev Cons (rev Lit h) lzRest))
      (arr (assocr . mapSnd (\(rev (h, t)) -> rev Cons h t)) >>>
       second (liftR split >>> second (updateR updateWindow' goLz77)) >>>
       arr (assocl . assocl) >>> first (arrInv (pinR (eqList eqChar) getMatch)) >>>
       arr (\(rev (p, lzRest)) -> rev Cons (rev Pair p) lzRest)))
  where
    def eqMaybeIntPair = eqMaybe (eqPair eqInt eqInt)

    def updateWindow =
      arrG (\(rev (w, h)) -> rev Cons h w) >>!
      arrG (pairl (new eqInt winLength)) >>! take

    def updateWindow' =
      arrG swap >>! revApp >>!
      arrG (pairl (new eqInt winLength)) >>! take
  end

sig maxMatch : GFun (List Symbol, (Symbol, List Symbol)) (Maybe (Int, Int))
def maxMatch =
  arrG (mapSnd (\(rev (h, t)) -> rev Cons h t)) >>!
  arrG (pairl (rev (new eqInt 0, new eqInt 0)) . pairl (rev Nil)) >>!
  traceG goMaxMatch >>! arrG (distr . mapFst is0) >>!
  caseG
    (erase >>! arrG (\(rev ()) -> rev Nothing))
    (arrG (rev Just))
  where
    def goMaxMatch =
      arrG (distr . mapFst uncons . shuffler . mapSnd shuffler) >>!
      (arrG unitl >>! fstG ++!
       arrG (assocl .
             mapSnd (shufflel . mapFst (mapFst (\(rev (wh, mb)) -> rev Cons wh mb) .
                                        shufflel) . assocl) . shuffler) >>!
       firstG (secondG copyG >>! arrG assocl >>! firstG updateMatch) >>!
       arrG (mapSnd (shuffler . swap) . assocr))

    sig updateMatch : GFun ((Int, Int), (List Symbol, List Symbol)) (Int, Int)
    def updateMatch =
      secondG (firstG (copyG >>! firstG length) >>!
               arrG assocr >>! secondG measureMatch) >>!
      arrG (inv eitherToBool . mapFst (assocl . mapSnd shuffler) . assocl .
            mapSnd (assocl . mapSnd ltIntC . shuffler) . assocr) >>!
      caseG sndG fstG

    sig measureMatch : GFun (List Symbol, List Symbol) Int
    def measureMatch =
      firstG copyG >>! arrG (mapSnd (assocr . pairr (new eqInt 0)) . assocr) >>!
      traceG goMeasureMatch
    def goMeasureMatch =
      arrG (distr . mapFst uncons) >>!
      caseG
        (arrG unitl >>! firstG copyG >>! arrG (rev Right . assocr))
        (arrG (distr . mapFst uncons . shuffler . mapSnd shuffler) >>!
          caseG
            (arrG unitl >>! sndG >>! sndG >>! arrG rev Left)
            (arrG (mapPair eqCharC assocr . assocr .
                   mapFst (assocr . mapFst (assocl . swap) . assocl) . assocl) >>!
             firstG sndG >>! arrG (inv eitherToBool . swap) >>!
             caseG
               (arrG (rev Right . mapSnd (mapSnd (mapSnd succ) . shuffler) . shuffler))
               (sndG >>! sndG >>! sndG >>! arrG rev Left)))
  end

sig getMatch : GFun (List Symbol, (Int, Int)) (List Symbol)
def getMatch =
  arrG (mapFst swap . assocl) >>! firstG revTake >>!
  arrG swap >>! repeatTake
  where
    sig revTake : GFun (Int, List a) (List a)
    def revTake = arrG (assocr . pairr (rev Nil)) >>! traceG goRevTake
    def goRevTake =
      arrG (distr . mapFst is0) >>!
      caseG
        (arrG unitl >>! sndG >>! arrG rev Left)
        (arrG (distr . mapFst uncons . shuffler) >>!
         (arrG unitl >>! sndG ++!
          arrG (mapPair pred (\(rev ((h, t), l)) -> rev (t, rev Cons h l)) . shuffler)))

    sig repeatTake : GFun (Int, List a) (List a)
    def repeatTake =
      secondG (copyG >>! arrG (assocr . pairr (rev Nil))) >>!
      traceG goRepeatTake >>! reverse
    def goRepeatTake =
      arrG (distr . mapFst is0) >>!
      (arrG unitl >>! sndG >>! sndG ++!
       arrG (distr . mapFst uncons . shuffler) >>!
       caseG
        (arrG unitl >>! secondG (firstG copyG >>! arrG assocr))
        (arrG (mapPair pred
                (shuffler . mapSnd (\(rev ((h, t), l)) -> rev (t, rev Cons h l)) .
                  shuffler) . shuffler)))
    def reverse = arrG (pairr (rev Nil)) >>! revApp
  end
